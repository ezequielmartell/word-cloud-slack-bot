{
    "display_information": {
        "name": "Word Cloud"
    },
    "features": {
        "bot_user": {
            "display_name": "Word Cloud",
            "always_online": false
        },
        "slash_commands": [
            {
                "command": "/wordcloud",
                "url": "https://wordcloudbot.ezdoes.xyz/slack/events",
                "description": "Creates a word cloud with in the called channel",
                "usage_hint": "[phrase][blank for usernames]",
                "should_escape": true
            }
        ]
    },
    "oauth_config": {
        "redirect_urls": [
            "https://wordcloudbot.ezdoes.xyz/slack/oauth_redirect"
        ],
        "scopes": {
            "user": [
                "chat:write"
            ],
            "bot": [
                "commands",
                "channels:history"
            ]
        }
    },
    "settings": {
        "interactivity": {
            "is_enabled": true,
            "request_url": "https://wordcloudbot.ezdoes.xyz/slack/events"
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": false,
        "token_rotation_enabled": false
    }
}